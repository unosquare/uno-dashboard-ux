{
    "name": "uno-dashboard-ux",
    "version": "4.48.0",
    "description": "Unosquare Dashboard UX/UI",
    "main": "dist/index.js",
    "files": [
        "dist",
        "tailwind.config.js"
    ],
    "dependencies": {
        "@fluentui/react-icons": "^2.0.260",
