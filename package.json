{
    "name": "uno-dashboard-ux",
    "version": "3.8.0",
    "description": "Unosquare Dashboard UX/UI",
    "main": "dist/index.js",
    "files": [
        "dist",
        "tailwind.config.js"
    ],
    "dependencies": {
        "@fluentui/react-icons": "^2.0.227",
        "@headlessui/react": "0.0.0-insiders.ce17c6d",
        "@tremor/react": "^3.14.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "recharts": "2.12.0",
        "tailwind-merge": "^2.2.1",
        "tailwind-styled-components": "^2.2.0",
        "uno-js": "^3.83.0",
        "uuid": "^9.0.1",
        "zustand": "^4.5.1"
    },
    "devDependencies": {
        "@rsbuild/core": "^0.4.2",
        "@rsbuild/plugin-react": "^0.4.2",
        "@testing-library/jest-dom": "^6.4.2",
        "@testing-library/react": "^14.2.1",
        "@types/jest": "^29.5.12",
        "@types/react": "^18.2.55",
        "@types/uuid": "^9.0.8",
        "@types/react-dom": "^18.2.19",
        "@types/recharts": "1.8.29",
        "@types/node": "^20.11.19",
        "@typescript-eslint/eslint-plugin": "^7.0.1",
        "autoprefixer": "^10.4.17",
        "eslint": "^8.56.0",
        "eslint-config-unosquare": "0.10.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "husky": "^9.0.11",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "lint-staged": "^15.2.2",
        "postcss-loader": "^8.1.0",
        "prettier": "^3.2.5",
        "tailwindcss": "^3.4.1",
        "ts-jest": "^29.1.2",
        "typescript": "^5.3.3"
    },
    "scripts": {
        "start": "rsbuild dev --open",
        "watch": "tsc --watch",
        "build": "tsc && npx ncp src/resources/ dist/resources",
        "lint": "tsc --noEmit && eslint ./src/**/*.{ts,tsx}",
        "lint:fix": "eslint ./src/**/*.{ts,tsx} --fix",
        "test": "jest --silent",
        "cest": "jest --silent --collectCoverage",
        "prepare": "husky"
    },
    "prettier": {
        "endOfLine": "auto",
        "semi": true,
        "trailingComma": "all",
        "singleQuote": true,
        "jsxSingleQuote": true,
        "printWidth": 120,
        "tabWidth": 4
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/unosquare/uno-dashboard-ux.git"
    },
    "lint-staged": {
        "*.{ts,html,tsx}": [
            "pnpm lint:fix"
        ]
    },
    "author": "Unosquare",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/unosquare/uno-dashboard-ux/issues"
    },
    "homepage": "https://github.com/unosquare/uno-dashboard-ux#readme"
}
