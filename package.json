        "recharts": "2.12.7",
        "tailwind-merge": "^2.5.3",
        "tailwind-styled-components": "^2.2.0",
        "uno-js": "^3.127.0",
        "uuid": "^10.0.0",
        "zustand": "^4.5.5"
    },
    "devDependencies": {
        "@rsbuild/core": "1.0.11",
        "@rsbuild/plugin-react": "1.0.3",
        "@testing-library/jest-dom": "^6.5.0",
        "@testing-library/react": "^16.0.1",
        "@types/jest": "^29.5.13",
        "@types/react": "^18.3.11",
        "@types/uuid": "^10.0.0",
        "@types/react-dom": "^18.3.0",
        "@types/recharts": "1.8.29",
        "@types/node": "^22.7.5",
        "@typescript-eslint/eslint-plugin": "7.18.0",
        "autoprefixer": "^10.4.20",
        "eslint": "^8.57.0",
        "eslint-config-unosquare": "0.20.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-react": "^7.37.1",
        "eslint-plugin-react-hooks": "^4.6.2",
        "husky": "^9.1.6",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "lint-staged": "^15.2.10",
        "postcss-loader": "^8.1.1",
        "prettier": "^3.3.3",
        "tailwindcss": "^3.4.13",
        "ts-jest": "^29.2.5",
        "typescript": "^5.6.2"
    },
    "scripts": {
        "start": "rsbuild dev --open",
        "watch": "tsc --watch",
        "build": "tsc && npx ncp src/resources/ dist/resources",
        "lint": "tsc --noEmit && eslint ./src/**/*.{ts,tsx}",
        "lint:fix": "eslint ./src/**/*.{ts,tsx} --fix",
        "test": "jest --silent",
        "cest": "jest --silent --collectCoverage",
        "prepare": "husky"
    },
    "prettier": {
        "endOfLine": "auto",
        "semi": true,
        "trailingComma": "all",
        "singleQuote": true,
        "jsxSingleQuote": true,
        "printWidth": 120,
        "tabWidth": 4
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/unosquare/uno-dashboard-ux.git"
    },
    "lint-staged": {
        "*.{ts,html,tsx}": [
            "pnpm lint:fix"
        ]
    },
