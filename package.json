        "react-dom": "^18.2.0",
        "recharts": "2.10.1",
        "tailwind-merge": "^2.0.0",
        "tailwind-styled-components": "^2.2.0",
        "uno-js": "^3.59.0"
    },
    "devDependencies": {
        "@rspack/cli": "0.3.14",
        "@testing-library/jest-dom": "^6.1.4",
        "@testing-library/react": "^14.1.2",
        "@types/jest": "^29.5.10",
        "@types/object-hash": "^3.0.6",
        "@types/react": "^18.2.38",
        "@types/react-dom": "^18.2.17",
        "@types/recharts": "1.8.28",
        "@types/node": "^20.10.0",
        "@typescript-eslint/eslint-plugin": "^6.12.0",
        "autoprefixer": "^10.4.16",
        "eslint": "^8.54.0",
        "eslint-config-unosquare": "0.8.2",
        "eslint-plugin-prettier": "^5.0.1",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "lint-staged": "^15.1.0",
        "object-hash": "^3.0.0",
        "postcss-loader": "^7.3.3",
        "prettier": "^3.1.0",
        "tailwindcss": "^3.3.5",
        "ts-jest": "^29.1.1",
        "typescript": "^5.3.2"
    },
    "scripts": {
        "start": "rspack serve",
        "watch": "tsc --watch",
        "build": "tsc && npx ncp src/resources/ dist/resources",
        "lint": "tsc --noEmit && eslint ./src/**/*.{ts,tsx}",
        "lint:fix": "eslint ./src/**/*.{ts,tsx} --fix",
        "test": "jest --silent",
        "cest": "jest --silent --collectCoverage",
        "prepare": "husky install"
    },
    "prettier": {
        "endOfLine": "auto",
        "semi": true,
        "trailingComma": "all",
        "singleQuote": true,
        "jsxSingleQuote": true,
        "printWidth": 120,
        "tabWidth": 4
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/unosquare/uno-dashboard-ux.git"
    },
    "lint-staged": {
        "*.{ts,html,tsx}": [
            "pnpm lint:fix"
        ]
    },
