{
    "name": "uno-dashboard-ux",
    "version": "2.40.0",
    "description": "Unosquare Dashboard UX/UI",
    "main": "dist/index.js",
    "files": [
        "dist",
        "tailwind.config.js"
    ],
    "dependencies": {
        "@fluentui/react-icons": "^2.0.210",
        "@tremor/react": "^3.6.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "recharts": "^2.7.3",
        "tailwind-merge": "^1.14.0",
        "tailwind-styled-components": "^2.2.0",
        "uno-js": "^3.24.0"
    },
    "devDependencies": {
        "@rspack/cli": "^0.2.11",
        "@types/object-hash": "^3.0.3",
        "@types/react": "^18.2.20",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.4.0",
        "autoprefixer": "^10.4.15",
        "eslint": "^8.47.0",
        "eslint-config-unosquare": "0.3.2",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-react": "^7.33.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "lint-staged": "^14.0.0",
        "object-hash": "^3.0.0",
        "postcss-loader": "^7.3.3",
        "prettier": "^3.0.1",
        "tailwindcss": "^3.3.3",
        "typescript": "^5.1.6",
        "husky": "^8.0.3"
    },
    "scripts": {
        "start": "rspack serve",
        "watch": "tsc --watch",
        "build": "tsc && npx ncp src/resources/ dist/resources",
        "lint": "tsc --noEmit && eslint ./{src,sample}/**/*.{ts,tsx}",
        "lint:fix": "eslint ./{src,sample}/**/*.{ts,tsx} --fix",
        "test": "tsc --noEmit",
        "prepare": "husky install"
    },
    "prettier": {
        "endOfLine": "auto",
        "semi": true,
        "trailingComma": "all",
        "singleQuote": true,
        "jsxSingleQuote": true,
        "printWidth": 120,
        "tabWidth": 4
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/unosquare/uno-dashboard-ux.git"
    },
    "lint-staged": {
        "*.{css,ts,html,tsx}": [
            "pnpm lint:fix"
        ]
    },
    "author": "Unosquare",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/unosquare/uno-dashboard-ux/issues"
    },
    "homepage": "https://github.com/unosquare/uno-dashboard-ux#readme"
}
