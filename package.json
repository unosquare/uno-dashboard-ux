{
    "name": "uno-dashboard-ux",
    "version": "2.13.0",
    "description": "Unosquare Dashboard UX/UI",
    "main": "dist/index.js",
    "files": [
        "dist",
        "tailwind.config.js"
    ],
    "dependencies": {
        "@fluentui/react-icons": "^2.0.204",
        "@tremor/react": "^3.3.0",
        "dayjs": "^1.11.9",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "recharts": "^2.7.2",
        "tailwind-merge": "^1.13.2",
        "tailwind-styled-components": "^2.2.0",
        "uno-js": "^3.12.0"
    },
    "devDependencies": {
        "@rspack/cli": "^0.2.4",
        "@types/object-hash": "^3.0.2",
        "@types/react-dom": "^18.2.6",
        "@typescript-eslint/eslint-plugin": "^5.60.1",
        "autoprefixer": "^10.4.14",
        "eslint": "^8.44.0",
        "eslint-config-unosquare": "^0.1.4y",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "husky": "^4.3.8",
        "lint-staged": "^13.2.3",
        "object-hash": "^3.0.0",
        "postcss-loader": "^7.3.3",
        "prettier": "^2.8.8",
        "tailwindcss": "^3.3.2",
        "typescript": "^5.1.6"
    },
    "scripts": {
        "start": "rspack serve",
        "build": "tsc && npx ncp src/resources/ dist/resources",
        "lint": "tsc --noEmit && eslint ./{src,sample}/**/*.{js,ts,tsx}",
        "lint:fix": "eslint ./{src,sample}/**/*.{js,ts,tsx} --fix",
        "test": "tsc --noEmit"
    },
    "prettier": {
        "endOfLine": "auto",
        "semi": true,
        "trailingComma": "all",
        "singleQuote": true,
        "jsxSingleQuote": true,
        "printWidth": 120,
        "tabWidth": 4
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/unosquare/uno-dashboard-ux.git"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{css,ts,html,tsx}": [
            "npm run lint:fix"
        ]
    },
    "author": "Unosquare",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/unosquare/uno-dashboard-ux/issues"
    },
    "homepage": "https://github.com/unosquare/uno-dashboard-ux#readme"
}
